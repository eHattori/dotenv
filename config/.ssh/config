Host github.com
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_github

Host gitlab.com
  HostName gitlab.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gitlab

Host github-episource
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_episource


